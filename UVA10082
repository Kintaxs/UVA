#include<iostream>
#include<string>
using namespace std;

int main()
{
	string a="`1234567890-=QWERTYUIOP[]\\ASDFGHJKL;'ZXCVBNM,./";
	string str;
	
	while(getline(cin,str))
	{
		for(int i=0;i<str.size();i++)
		{
			if(str[i]==' ')
				cout<<" ";
			else
				cout<<a[a.find(str[i])-1];
		}
		
		cout<<endl;
	}
	

}
